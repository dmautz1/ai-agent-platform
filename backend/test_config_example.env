# Environment variables for testing Supabase integration
# Copy this file to .env.test and update with your test database credentials

# Environment
ENVIRONMENT=testing
DEBUG=true

# Test Supabase Configuration (Use a separate test project/database)
SUPABASE_URL=https://your-test-project.supabase.co
SUPABASE_KEY=your-test-anon-key
SUPABASE_SERVICE_KEY=your-test-service-key

# Logging Configuration for Tests
LOG_LEVEL=DEBUG
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Test-specific settings
MAX_CONCURRENT_JOBS=5
JOB_TIMEOUT=60
JOB_RETRY_ATTEMPTS=1

# Security settings (not critical for testing)
JWT_SECRET=test-jwt-secret-key-for-testing-only
API_RATE_LIMIT=1000/minute

# CORS settings for test environment
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:5173 